{
  "manifest_version": 2,
  "name": "Hide Tweet Views",
  "version": "1.0.2",
  "description": "Hides tweet views.",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/*"],
      "js": ["script.js"]
    }
  ]
}
